@model List<TblHakkinda>

<div class="resume-section-content">
    @foreach (var x in Model)
    {
    <h1 class="mb-0">
        @x.Ad
        <span class="text-primary">@x.Soyad</span>
    </h1>
    <div class="subheading mb-5">
        @x.Adres · @x.Telefon ·
        <a href="mailto:name@email.com">@x.Mail</a>
    </div>
    <p class="lead mb-5">@x.Aciklama</p>
        @await Html.PartialAsync("SosyalMedya", ViewData["SosyalMedya"])
    }
</div>

@model List<TblIletisim>

<div class="resume-section-content">
    <h2 class="mb-5">İletişim</h2>

    <form method="post" asp-action="Iletisim" asp-controller="Default">
        <label>Ad Soyad</label>
        <input class="form-control" type="text" name="AdSoyad"/>

        <label>Mail</label>
        <input class="form-control" type="text" name="Mail" />

        <label>Konu</label>
        <input class="form-control" type="text" name="Konu" />

        <label>Mesaj</label>
        <input class="form-control" type="text" name="Mesaj" />

        <br />
        <button class="btn btn-info" type="submit">Gönder</button>
    </form>
</div>
